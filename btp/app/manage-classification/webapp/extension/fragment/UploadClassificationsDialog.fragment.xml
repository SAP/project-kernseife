<core:FragmentDefinition xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns="sap.m">
    <Dialog id="ImportDialogId" title="{i18n>import}" class="sapUiResponsiveContentPadding" beforeOpen=".onBeforeOpen" afterClose=".onAfterClose">
        <content>
            <u:FileUploader id="uploader" fileType="xls,xlsx" multiple="false" fileAllowed=".onFileAllowed" fileEmpty=".onFileEmpty" uploadComplete=".onUploadComplete" typeMissmatch=".onTypeMismatch" sendXHR="true" useMultipart="false" placeholder="{i18n>importPlaceholder}" httpRequestMethod="Put"/>
        </content>
        <beginButton>
            <Button id="ok" text="{i18n>OK}" press=".onOk" type="Emphasized" enabled="false"/>
        </beginButton>
        <endButton>
            <Button id="cancel" text="{i18n>cancel}" press=".onCancel"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>
