{
    "name": "manage-classification",
    "version": "1.0.1",
    "description": "Classification App",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "build": "ui5 build preload --clean-dest --include-task=generateCachebusterInfo",
        "start": "ui5 serve",
        "test": "npm run test:node && npm run test:java",
        "test:java": "karma start --server=java --single-run"
    },
    "devDependencies": {
        "@sap/eslint-plugin-ui5-jsdocs": "2.0.8",
        "@sap/ux-specification": "^1.124.14",
        "@sap/ux-ui5-tooling": "1.17.3",
        "@ui5/cli": "^4.0.14",
        "eslint": "9.25.1",
        "ui5-task-zipper": "^3.4.1"
    }
}
